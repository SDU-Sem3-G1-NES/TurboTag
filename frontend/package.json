{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "genapi": "dotnet restore ../TurboTag.sln && dotnet build ../TurboTag.sln && swagger tofile --output swagger.json ../API/bin/Debug/net9.0/API.dll v1 && nswag run nswag.json",
    "devapi": "npm run genapi && vite",
    "build": "npm run genapi && tsc -b && vite build",
    "lint": "eslint .",
    "lint-fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier-fix": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "antd": "^5.24.6",
    "api": "^6.1.2",
    "axios": "^1.8.2",
    "bootstrap": "^5.3.3",
    "crypto-js": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-notifications-component": "^4.0.1",
    "react-router-dom": "^7.6.0",
    "react-tag-input-component": "^2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.3.4"
  }
